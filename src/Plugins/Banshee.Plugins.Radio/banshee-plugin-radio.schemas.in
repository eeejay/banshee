<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/banshee/plugins/radio/always_show_helix_stations</key>
      <applyto>/apps/banshee/plugins/radio/always_show_helix_stations</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Show stations requiring Helix/RealPlayer</short>
        <long>Always show stations that require the Helix/RealPlayer engine, even if the engine is not loaded.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/plugins/radio/enabled</key>
      <applyto>/apps/banshee/plugins/radio/enabled</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Plugin enabled</short>
        <long>Radio plugin enabled</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/plugins/radio/last_update_check</key>
      <applyto>/apps/banshee/plugins/radio/last_update_check</applyto>
      <owner>banshee</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>Time of the last radio update check</short>
        <long>Last time the master station list was checked for updates</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/plugins/radio/last_updated</key>
      <applyto>/apps/banshee/plugins/radio/last_updated</applyto>
      <owner>banshee</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>Time of the last radio update</short>
        <long>Last time XSPF stations were updated from radio.banshee-project.org</long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
