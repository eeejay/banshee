<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/banshee/plugins/audioscrobbler/enabled</key>
      <applyto>/apps/banshee/plugins/audioscrobbler/enabled</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Plugin enabled</short>
        <long>Audioscrobbler reporting plugin enabled</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/plugins/audioscrobbler/username</key>
      <applyto>/apps/banshee/plugins/audioscrobbler/username</applyto>
      <owner>banshee</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>Username</short>
        <long>last.fm Username</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/plugins/audioscrobbler/password</key>
      <applyto>/apps/banshee/plugins/audioscrobbler/password</applyto>
      <owner>banshee</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>Password</short>
        <long>last.fm Password</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/plugins/audioscrobbler/engine_enabled</key>
      <applyto>/apps/banshee/plugins/audioscrobbler/engine_enabled</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Engine enabled</short>
        <long>Audioscrobbler reporting engine enabled</long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
