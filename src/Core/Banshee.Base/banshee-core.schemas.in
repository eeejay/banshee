<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/banshee/core/io_backend</key>
      <applyto>/apps/banshee/core/io_backend</applyto>
      <owner>banshee</owner>
      <type>string</type>
      <default>unix</default>
      <locale name="C">
        <short>Set the IO backend in Banshee</short>
        <long>Can be either "systemio" (.NET System.IO), "unix" (Native Unix), or "gnomevfs" (GNOME VFS); takes effect on Banshee start (restart necessary)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/import/audio_cd_error_correction</key>
      <applyto>/apps/banshee/import/audio_cd_error_correction</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Enable error correction</short>
        <long>When importing an audio CD, enable error correction (paranoia mode)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/import/show_initial_import_dialog</key>
      <applyto>/apps/banshee/import/show_initial_import_dialog</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Show the Initial Import Dialog</short>
        <long>Show the Initial Import Dialog when the Banshee library is empty</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/library/base_location</key>
      <applyto>/apps/banshee/library/base_location</applyto>
      <owner>banshee</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>Library location</short>
        <long>Base location for library music</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/library/copy_on_import</key>
      <applyto>/apps/banshee/library/copy_on_import</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Copy music on import</short>
        <long>Copy and rename music to banshee music library directory when importing</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/library/file_pattern</key>
      <applyto>/apps/banshee/library/file_pattern</applyto>
      <owner>banshee</owner>
      <type>string</type>
      <default>%track_number%. %title%</default>
      <locale name="C">
        <short>Library File Pattern</short>
        <long>Format for creating a track filename inside the library. Do not use path tokens/characters here. See LibraryFolderPattern. Legal tokens: %artist%, %album%, %title%, %track_number%, %track_count%, %track_number_nz% (No prefixed zero), %track_count_nz% (No prefixed zero).</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/library/folder_pattern</key>
      <applyto>/apps/banshee/library/folder_pattern</applyto>
      <owner>banshee</owner>
      <type>string</type>
      <default>%artist%%path_sep%%album%</default>
      <locale name="C">
        <short>Library Folder Pattern</short>
        <long>Format for creating a track folder inside the library. Do not create an absolute path. Path here is relative to the Banshee music directory. See LibraryLocation. Legal tokens: %artist%, %album%, %title%, %track_number%, %track_count%, %track_number_nz% (No prefixed zero), %track_count_nz% (No prefixed zero), %path_sep% (portable directory separator (/)).</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/library/move_on_info_save</key>
      <applyto>/apps/banshee/library/move_on_info_save</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Move music on info save</short>
        <long>Move music within banshee music library directory when saving track info</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/library/playlist_sort_criteria</key>
      <applyto>/apps/banshee/library/playlist_sort_criteria</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>Sort criteria of playlists</short>
        <long>Sort criteria of library playlists in the source view (0 = Name, 1 = Size)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/library/playlist_sort_order</key>
      <applyto>/apps/banshee/library/playlist_sort_order</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>Sort order of playlists</short>
        <long>Sort order of library playlists in the source view (0 = Ascending, 1 = Descending)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/library/sort_column</key>
      <applyto>/apps/banshee/library/sort_column</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>-1</default>
      <locale name="C">
        <short>Column index</short>
        <long>Column index for sorting the library source. -1 for unset.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/library/sort_type</key>
      <applyto>/apps/banshee/library/sort_type</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>Column sort type</short>
        <long>Column sort type for the library source. Ascending (0) or Descending (1)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/library/write_metadata</key>
      <applyto>/apps/banshee/library/write_metadata</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Write metadata back to audio files</short>
        <long>If enabled, metadata (tags) will be written back to audio files when using the track metadata editor.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/playback/repeat</key>
      <applyto>/apps/banshee/playback/repeat</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>Repeat playback</short>
        <long>Repeat mode (0 = None, 1 = All, 2 = Single)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/playback/shuffle</key>
      <applyto>/apps/banshee/playback/shuffle</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Shuffle playback</short>
        <long>Enable shuffle mode</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/player_engine/backend</key>
      <applyto>/apps/banshee/player_engine/backend</applyto>
      <owner>banshee</owner>
      <type>string</type>
      <default>helix-remote</default>
      <locale name="C">
        <short>Backend</short>
        <long>Name of media playback engine backend</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/player_engine/volume</key>
      <applyto>/apps/banshee/player_engine/volume</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>80</default>
      <locale name="C">
        <short>Volume</short>
        <long>Volume of playback relative to mixer output</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/player_window/height</key>
      <applyto>/apps/banshee/player_window/height</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>700</default>
      <locale name="C">
        <short>Window Height</short>
        <long>Height of the main interface window.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/player_window/last_file_chooser_uri</key>
      <applyto>/apps/banshee/player_window/last_file_chooser_uri</applyto>
      <owner>banshee</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>URI</short>
        <long>URI of last file folder</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/player_window/maximized</key>
      <applyto>/apps/banshee/player_window/maximized</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Window Maximized</short>
        <long>True if main window is to be maximized, false if it is not.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/player_window/open_location_history</key>
      <applyto>/apps/banshee/player_window/open_location_history</applyto>
      <owner>banshee</owner>
      <type>list</type>
      <list_type>string</list_type>
      <default>[]</default>
      <locale name="C">
        <short>URI List</short>
        <long>List of URIs in the history drop-down for the open location dialog</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/player_window/show_cover_art</key>
      <applyto>/apps/banshee/player_window/show_cover_art</applyto>
      <owner>banshee</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Show cover art</short>
        <long>Show cover art below source view if available</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/player_window/source_view_width</key>
      <applyto>/apps/banshee/player_window/source_view_width</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>175</default>
      <locale name="C">
        <short>Source View Width</short>
        <long>Width of Source View Column.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/player_window/width</key>
      <applyto>/apps/banshee/player_window/width</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>1024</default>
      <locale name="C">
        <short>Window Width</short>
        <long>Width of the main interface window.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/player_window/x_pos</key>
      <applyto>/apps/banshee/player_window/x_pos</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>Window Position X</short>
        <long>Pixel position of Main Player Window on the X Axis</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/banshee/player_window/y_pos</key>
      <applyto>/apps/banshee/player_window/y_pos</applyto>
      <owner>banshee</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>Window Position Y</short>
        <long>Pixel position of Main Player Window on the Y Axis</long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
